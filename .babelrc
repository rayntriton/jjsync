{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties",
      [ "module-resolver", {
        "root": [
        "./"
        ],
        "alias": {
          "~root": "./src/",
          "~types": "./src/types/"
        } } ]
    ]
}
